{
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"cSpell.words": [
		"vocli",
		"commitlint",
		"echarts",
		"stylelint",
		"browserslist",
		"qrcode",
		"alicdn",
		"camelcase",
		"cssnano",
		"photoswipe",
		"nodenext",
		"esbuild",
		"outfile",
		"outdir"
	],
	"files.associations": {
		"*.cjson": "jsonc",
		"*.json": "jsonc"
	}
}
